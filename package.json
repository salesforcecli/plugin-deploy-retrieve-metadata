{
  "name": "@salesforce/plugin-project-org",
  "description": "Commands for deploying source to salesforce orgs",
  "version": "0.0.2",
  "author": "Salesforce",
  "bugs": "https://github.com/forcedotcom/cli/issues",
  "dependencies": {
    "@oclif/core": "^0.5.18",
    "@salesforce/core": "3.1.0-v3.0",
    "@salesforce/plugin-project-utils": "^0.0.7",
    "@salesforce/source-deploy-retrieve": "^3.0.0",
    "chalk": "^4.1.1",
    "cli-ux": "^5.5.1",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/plugin-command-snapshot": "^2.2.0",
    "@salesforce/cli-plugins-testkit": "^0.0.25",
    "@salesforce/dev-config": "^2.1.2",
    "@salesforce/dev-scripts": "^0.9.17",
    "@salesforce/plugin-command-reference": "^1.3.3",
    "@salesforce/plugin-config": "^1.2.6",
    "@salesforce/plugin-project": "^0.0.5",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/source-testkit": "^0.0.6",
    "@salesforce/ts-sinon": "1.3.5",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "chai": "^4.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^27.0.3",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.3.8",
    "lint-staged": "^10.2.2",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "oclif": "^2.0.0-main.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "shx": "0.2.2",
    "sinon": "10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/oclif.manifest.json",
    "/schemas"
  ],
  "homepage": "https://github.com/salesforcecli/plugin-project-org",
  "keywords": [
    "force",
    "salesforce",
    "sf"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "hooks": {
      "project:findDeployers": "./lib/hooks/findDeployers"
    },
    "bin": "sf",
    "topicSeparator": " ",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference",
      "@salesforce/plugin-config",
      "@salesforce/plugin-project"
    ],
    "topics": {
      "project": {
        "description": "Commands to interact with projects",
        "subtopics": {
          "deploy": {
            "description": "Commands to deploy source to and retrieve source from salesforce orgs"
          }
        }
      }
    }
  },
  "repository": "salesforcecli/plugin-project-org",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:json-schema && yarn test:command-reference",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/dev commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/dev snapshot:compare",
    "test:json-schema": "./bin/dev schema:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "version": "oclif readme"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sf-husky-pre-commit",
      "commit-msg": "sf-husky-commit-msg",
      "pre-push": "sf-husky-pre-push"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
